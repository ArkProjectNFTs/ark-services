{
  "name": "@ark-app/admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "git clean -xdf .next node_modules",
    "db:generate": "pnpm prisma generate && pnpm prisma format",
    "db:up": "COMPOSE_PROJECT_NAME=ark-admin docker compose up -d || docker-compose -p up -d",
    "db:setup": "pnpm db:up && pnpm db:migrate && pnpm db:seed",
    "db:nuke": "COMPOSE_PROJECT_NAME=ark-admin docker compose down --volumes --remove-orphans || docker-compose down --volumes --remove-orphans",
    "db:reset": "pnpm db:nuke && pnpm db:setup",
    "db:seed": "pnpm prisma db seed",
    "db:studio": "pnpm prisma studio",
    "db:migrate": "pnpm with-env prisma migrate dev",
    "db:deploy": "pnpm prisma migrate deploy",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@auth/core": "^0.16.1",
    "@auth/prisma-adapter": "^1.0.3",
    "@aws-sdk/client-api-gateway": "^3.405.0",
    "@aws-sdk/client-dynamodb": "^3.435.0",
    "@aws-sdk/client-ecs": "^3.429.0",
    "@aws-sdk/credential-provider-node": "^3.405.0",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.4.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-email/components": "^0.0.7",
    "@react-email/tailwind": "^0.0.9",
    "@t3-oss/env-nextjs": "^0.6.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tanstack/react-query": "^4.32.0",
    "@tanstack/react-query-devtools": "^4.32.0",
    "@tanstack/react-query-next-experimental": "5.0.0-alpha.80",
    "@tanstack/react-table": "^8.9.3",
    "@trpc/client": "^10.36.0",
    "@trpc/next": "^10.36.0",
    "@trpc/react-query": "^10.36.0",
    "@trpc/server": "^10.36.0",
    "axios": "^1.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.268.0",
    "moment": "^2.29.4",
    "next": "^13.4.10",
    "next-auth": "^4.23.2",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.4",
    "react": "^18.2.0",
    "react-day-picker": "^8.9.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.0",
    "react-use-clipboard": "^1.0.9",
    "resend": "^1.1.0",
    "starknet": "^5.19.5",
    "superjson": "^1.13.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/eslint": "^8.44.4",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.5.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.4.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "ts-node --transpile-only ./prisma/seed.ts"
  },
  "lint-staged": {
    "(apps|packages)/**/*.{js,cjs,mjs,ts,tsx,md,json}": [
      "pnpm lint typecheck"
    ]
  }
}
